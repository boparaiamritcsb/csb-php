{
  "name": "csb/php",
  "description": "CSB Package for PHP applications.",
  "keywords": [
    "csb",
    "package",
    "php"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Amritpal Singh",
      "email": "amrit@customersuccessbox.com"
    }
  ],
  "require": {
    "php": ">=5.4",
    "ext-json": "*",
    "ext-dom": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4"
  },
  "autoload": {
    "psr-4": {
      "CSB\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
